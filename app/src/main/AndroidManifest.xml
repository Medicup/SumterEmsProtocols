<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.red6tech.android.scfems2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AsthmaActivity"
            android:label="@string/title_activity_asthma2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CarbonMonoxideActivity"
            android:label="@string/title_activity_carbon_monoxide"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CopdActivity"
            android:label="@string/title_activity_copd"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DifficultAirwayActivity"
            android:label="@string/title_activity_difficult_airway"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>