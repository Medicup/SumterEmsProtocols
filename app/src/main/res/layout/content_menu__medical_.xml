<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.red6tech.android.scfems2.Parameters.MedicalEmergency.Menu_Medical_Activity"
    tools:showIn="@layout/activity_menu__medical_">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.red6tech.android.scfems2.Parameters.Cardiac.Menu_Card_Activity"
        tools:showIn="@layout/activity_menu__card_">

        <LinearLayout
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/but_abdpain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_abdominal_pain"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_allergic_reaction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_allergic_reaction"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_ams"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_ams"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_drug_od"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_drug_overdose"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_environ_cold"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_environmental_cold"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_environ_heat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_environmental_heat"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_esop_fbao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_esophageal_foreign_body_obstruction"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_excied_delirium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_excited_delirium"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_extra_pyramidal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_extra_pyramidal"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_hazmat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_hazardous_material"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_htn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_hypertensive_crisis"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_hypohyperglycemia"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_hypo_hyper_glycemia"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_nausea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_nausea_vomiting"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_nonenviron_hyper"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_non_environmental_hyperthermia"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_psyhological"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_psychological_behavioral"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_scene_rehab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_scene_rehab"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_seizure"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_seizure"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_sepsis"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_sepsis"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_shock"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_shock"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_sicklecell"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_sickle_cell_anemia"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/but_syncope"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_syncope"
                android:layout_weight="1"/>
        </LinearLayout>

    </android.support.constraint.ConstraintLayout>
</android.support.v4.widget.NestedScrollView>
